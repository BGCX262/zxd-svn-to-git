<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ec="http://schemas.microsoft.com/expression/2010/controls" x:Class="ZxdFramework.App.Shell"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400" KeyDown="UserControl_KeyDown" Loaded="UserControl_Loaded">
    
    <Grid x:Name="LayoutRoot" Background="White">
        <ContentControl x:Name="shellContent" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
        	<ec:PathListBox HorizontalAlignment="Left" Height="70" Margin="72,64,0,0" VerticalAlignment="Top" Width="113"/>
        </ContentControl>
    </Grid>
</UserControl>
